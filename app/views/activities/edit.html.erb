<h1>Edit Here!</h1>

<%= form_for @activity do |a| %>
	Name:<%= a.text_field :name %>
	Date: <%= a.text_field :date %>
	Location: <%= a.text_field :location %>
	Headline: <%= a.text_field :headline %>
	Description: <%= a.text_field :description %>
	<%= a.submit "Submit these changes!" %>
<%end%>

<%= link_to "Back to Activities", activities_path %>